{
  "expo": {
    "name": "Split Fair",
    "slug": "split-fair",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/base_app_icon.png", 
    "splash": {
      "image": "./src/assets/splash.png", 
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.splitfair.app",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {

        "foregroundImage": "./src/assets/icons/android_foreground.png",
        

        "monochromeImage": "./src/assets/icons/android_monochrome.png",
        
        "backgroundColor": "#000000"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.splitfair.app"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-google-signin/google-signin",
      [
        "expo-build-properties",
        {
          "ios": { "useFrameworks": "static" }
        }
      ],
      "expo-asset",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
